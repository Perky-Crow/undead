<title><%= page_title %></title>

<meta name="viewport" content="width=device-width,initial-scale=1">
<meta name="description" content="<%= page_description %>">
<meta name="keywords" content="<%= page_keywords %>">
<meta name="author" content="<%= page_author %>">

<%= favicon_link_tag 'favicon.ico', rel: 'icon', type: 'image/x-icon', 'data-turbo-track': 'reload' %>
<%= favicon_link_tag 'favicon.ico', rel: 'shortcut icon', type: 'image/x-icon', 'data-turbo-track': 'reload' %>
<%= favicon_link_tag 'apple-touch-icon.png', rel: 'apple-touch-icon', type: 'image/png', 'data-turbo-track': 'reload' %>
<%= favicon_link_tag 'android-chrome-192x192.png', rel: 'icon', sizes: '192x192', type: 'image/png', 'data-turbo-track': 'reload' %>
<%= favicon_link_tag 'android-chrome-256x256.png', rel: 'icon', sizes: '256x256', type: 'image/png', 'data-turbo-track': 'reload' %>

<%= csrf_meta_tags %>
<%= csp_meta_tag %>

<%= stylesheet_link_tag "https://fonts.googleapis.com/css2?family=Average&family=Rye&family=Source+Code+Pro:wght@400;600&display=swap", "data-turbo-track": "reload" %>
<%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
<%= javascript_importmap_tags %>
